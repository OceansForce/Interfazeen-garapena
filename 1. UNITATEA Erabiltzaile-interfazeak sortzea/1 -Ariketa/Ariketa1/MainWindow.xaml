        <TextBox x:Name="textBox4" HorizontalAlignment="Left" Margin="200,182,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.709,1.365" TextChanged="TextBox_TextChanged_4"/>
        <TextBox IsReadOnly="True" x:Name="textBox5" HorizontalAlignment="Left" Margin="200,246,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.709,1.365" TextChanged="TextBox_TextChanged_5"/>
       
