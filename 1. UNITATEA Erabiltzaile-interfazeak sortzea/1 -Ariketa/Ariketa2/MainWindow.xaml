<Window x:Class="Ariketa2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ariketa2"
        mc:Ignorable="d"
        Title="MainWindow" Height="330" Width="400">
    <Grid>
        <Button x:Name="Frase1" Content="Frase1" HorizontalAlignment="Left" Margin="46,119,0,0" VerticalAlignment="Top" Height="30" Width="61" Click="Button_Click"/>
        <Button x:Name="Frase2" Content="Frase2" HorizontalAlignment="Left" Margin="165,119,0,0" VerticalAlignment="Top" Height="30" Width="62" IsEnabled="False" Click="Button_Click_1" />
        <Button x:Name="Frase3" Content="Frase3" HorizontalAlignment="Left" Margin="290,119,0,0" VerticalAlignment="Top" Height="30" Width="62" IsEnabled="False" Click="Button_Click_2"/>
        <Button x:Name="Frase4" Content="Frase4" HorizontalAlignment="Left" Margin="46,171,0,0" VerticalAlignment="Top"  Height="30" Width="61" IsEnabled="False" Click="Button_Click_3"/>
        <Button x:Name="Frase5" Content="Frase5" HorizontalAlignment="Left" Margin="166,171,0,0" VerticalAlignment="Top"  Height="30" Width="61" IsEnabled="False" Click="Button_Click_4"/>
        <Button x:Name="Unir" Content="Unir" HorizontalAlignment="Left" Margin="290,166,0,0" VerticalAlignment="Top" Height="30" Width="61" IsEnabled="False" Click="Button_Click_5"/>
        <Button Content="Limpiar" HorizontalAlignment="Center" Margin="0,210,0,0" VerticalAlignment="Top" Height="30" Width="308" Click="Button_Click_6"/>
        <Button Content="Salir" HorizontalAlignment="Center" Margin="0,245,0,0" VerticalAlignment="Top" Height="30" Width="308" Click="Button_Click_7"/>
        <TextBox x:Name="Text" HorizontalAlignment="Center" Margin="0,58,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="308" Height="26" TextChanged="TextBox_TextChanged"/>

    </Grid>
</Window>
